{
  "name": "plumthedev/php-cloudwatch-logger",
  "description": "A PHP library for logging to AWS Cloudwatch.",
  "type": "library",
  "keywords": [
    "aws",
    "cloudwatch",
    "logger",
    "php"
  ],
  "homepage": "https://github.com/plumthedev/php-cloudwatch-logger",
  "license": "MIT",
  "authors": [
    {
      "name": "plumthedev",
      "email": "kacper.pruszynski99@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^8.1",
    "aws/aws-sdk-php": "^3",
    "webmozart/assert": "^1.11",
    "phpnexus/cwh": "^3.1",
    "psr/container": "^2.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit": "^10",
    "mockery/mockery": "^1.6",
    "slevomat/coding-standard": "^8.14",
    "phpstan/phpstan": "^1.10",
    "phpstan/phpstan-mockery": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "CloudwatchLogger\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\CloudwatchLogger\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  }
}
